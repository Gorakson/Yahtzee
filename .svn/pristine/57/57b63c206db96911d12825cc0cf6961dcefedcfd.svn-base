package edu.hm.cs.kniffel.model.full;

public class Dice {
	
	public static final int MIN_VALUE = 1;
	public static final int MAX_VALUE = 6;
	
	private int value;
	
	public int getValue() {
		return value;
	}
	
	public void roll() {
        value = (int) (Math.random() * MAX_VALUE + MIN_VALUE);
	}
}
